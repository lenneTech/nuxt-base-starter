{
  "name": "nuxt-base-template",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "node": "./tslib.es6.mjs"
      }
    }
  },
  "scripts": {
    "c": "pnpm run check",
    "check": "pnpm audit && pnpm run format:check && pnpm run lint && pnpm test && pnpm run build && pnpm start",
    "check:fix": "pnpm install && pnpm audit --fix && pnpm run format && pnpm run lint:fix && pnpm test && pnpm run build && pnpm start",
    "check:naf": "pnpm install && pnpm run format && pnpm run lint:fix && pnpm test && pnpm run build && pnpm start",
    "cf": "pnpm run check:fix",
    "cnaf": "pnpm run check:naf",
    "init": "pnpm install",
    "reinit": "npx rimraf pnpm-lock.yaml && npx nuxt cleanup && npx rimraf node_modules && pnpm install",
    "clean": "npx rimraf .nuxt .output",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "build:develop": "npx cross-env NODE_ENV=development nuxt build",
    "build:test": "npx cross-env NODE_ENV=test nuxt build",
    "build:prod": "npx cross-env NODE_ENV=production nuxt build",
    "start": "nuxt dev",
    "start:develop": "node .output/server/index.mjs",
    "start:test": "node .output/server/index.mjs",
    "start:prod": "node .output/server/index.mjs",
    "start:tunnel": "nuxt dev --tunnel",
    "start:extern": "npx cross-env HOST=0.0.0.0 nuxt dev",
    "generate": "nuxt generate",
    "generate-types": "openapi-ts",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "simple-git-hooks",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:all": "pnpm run test:unit && pnpm run test:e2e",
    "lint": "oxlint app/",
    "lint:fix": "oxlint --fix --fix-suggestions app/",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "fix": "pnpm run lint:fix && pnpm run format"
  },
  "dependencies": {
    "@better-auth/passkey": "1.4.10",
    "@hey-api/client-fetch": "0.13.1",
    "@lenne.tech/bug.lt": "latest",
    "@lenne.tech/nuxt-extensions": "1.2.9",
    "@nuxt/image": "2.0.0",
    "@nuxt/ui": "4.3.0",
    "@pinia/nuxt": "0.11.3",
    "@vueuse/nuxt": "14.1.0",
    "better-auth": "1.4.10",
    "qrcode": "1.5.4",
    "tus-js-client": "4.3.1",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.90.3",
    "@iconify-json/lucide": "1.2.88",
    "@nuxt/devtools": "3.1.1",
    "@nuxt/test-utils": "3.23.0",
    "@nuxtjs/color-mode": "4.0.0",
    "@nuxtjs/plausible": "2.0.1",
    "@nuxtjs/seo": "3.3.0",
    "@playwright/test": "1.57.0",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "25.0.6",
    "@types/qrcode": "1.5.6",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/test-utils": "2.4.6",
    "dayjs-nuxt": "2.1.11",
    "happy-dom": "20.3.7",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.7",
    "mongodb": "7.1.0",
    "nuxt": "4.2.2",
    "oxfmt": "latest",
    "oxlint": "latest",
    "rimraf": "6.1.2",
    "simple-git-hooks": "2.13.1",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "pnpm run test:unit"
  },
  "lint-staged": {
    "app/**/*.{ts,vue}": [
      "oxlint --fix",
      "oxfmt"
    ],
    "tests/**/*.ts": [
      "oxlint --fix"
    ]
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "simple-git-hooks",
      "vue-demi"
    ]
  }
}
